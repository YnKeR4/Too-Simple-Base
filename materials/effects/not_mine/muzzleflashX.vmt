"UnlitGeneric"
{
	"$basetexture" "effects/not_mine/muzzleflashX"
	"$additive" "3"
	"$vertexcolor" "3"
	"$vertexalpha" "0.2"
   "Proxies"
   {
      "AnimatedTexture"
      {
         "animatedtexturevar" "$basetexture"
         "animatedtextureframenumvar" "$frame"
         "animatedtextureframerate" 15
      }
   }
}